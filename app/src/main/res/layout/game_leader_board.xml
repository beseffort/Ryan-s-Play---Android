<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ryansplayllc.ryansplay.GameLeaderBoardActivity" >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="95dp"
        android:id="@+id/game_leader_board_header"
        android:background="#347296">
        <include
            android:id="@+id/header"
            layout="@layout/header" />


        <fonts.BebasNeueBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="SL Bees Vs 51S    "
            android:id="@+id/gl_game_name"
            android:textColor="#FFFFFF"
            android:textSize="17dp"
            android:textStyle="bold"
            android:layout_below="@+id/header"
            android:layout_alignLeft="@+id/gl_username"
            android:layout_alignStart="@+id/gl_username"
            />

        <fonts.BebasNeueBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="RyanWheeler"
            android:id="@+id/gl_username"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:textSize="10dp"
            android:layout_below="@+id/textView60"
            android:layout_marginLeft="10dp"
            />

        <fonts.BebasNeueBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView60"
            android:textColor="#FFFFFF"
            android:textSize="16dp"
            android:textStyle="bold"
            android:layout_alignTop="@+id/gl_game_name"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="10dp"
            android:layout_marginEnd="22dp" />

        <fonts.BebasNeueBoldTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="5 Plays"
            android:id="@+id/fgl_game_length"
            android:textSize="17dp"
            android:layout_below="@+id/header"
            android:layout_toLeftOf="@+id/textView60"
            android:layout_toStartOf="@+id/textView60"
            android:textColor="#ffffffff"
            android:textStyle="bold"/>


    </RelativeLayout>
    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/glb_ln_fotter"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/game_leader_board_header" >

    </ScrollView>

    <include
        android:id="@+id/glb_ln_fotter"
        layout="@layout/footer" />

    <fonts.BebasNeueBoldTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Final LeaderBoard"
        android:id="@+id/textView56"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:textSize="21dp"
        android:textColor="#ffffffff"
        android:layout_marginTop="15dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rpicon"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView56"
        android:layout_marginRight="10dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/game_leader_board_header"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/lb_returntohome"
        android:weightSum="1">

        <RelativeLayout
            android:id="@+id/gh_rl_player_info"
            android:layout_width="fill_parent"
            android:layout_height="85dp"
            android:background="#f6f7f2">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:orientation="vertical"
                android:padding="@dimen/default_padding"
                android:id="@+id/linearLayout3"
                android:layout_marginLeft="60dp">

                <TextView
                    android:id="@+id/my_tv_player_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#004770"
                    android:textSize="13dp"
                    android:layout_marginTop="5dp"
                    android:text="RyanWheeler"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="#1"
                    android:id="@+id/playerRank"

                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:textSize="13dp"
                    android:textColor="#999999" />
            </LinearLayout>


            <TextView
                android:id="@+id/fgl_player_total_plays"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#999999"
                android:textSize="13dp"

                android:text="5"
                android:layout_below="@+id/textView64"
                android:layout_alignLeft="@+id/textView64"
                android:layout_alignStart="@+id/textView64"
                 />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/play"
                android:id="@+id/textView64"
                android:textSize="9dp"
                android:textColor="#347296"
                android:layout_toLeftOf="@+id/textView66"
                android:layout_toStartOf="@+id/textView66"
                android:layout_marginRight="30dp"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="16dp"
                android:layout_alignTop="@+id/pl_tv_player_name"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/points"
                android:id="@+id/textView66"
                android:textColor="#347296"
                android:textSize="9dp"
                android:layout_marginRight="18dp"
                android:layout_alignBottom="@+id/textView64"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="18dp" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="0"
                android:id="@+id/playerPoints"
                android:textSize="13dp"
                android:layout_alignTop="@+id/fgl_player_total_plays"
                android:layout_alignLeft="@+id/textView66"
                android:layout_alignStart="@+id/textView66"
                android:layout_marginTop="1dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Share on Facebook"
                android:id="@+id/fl_fbshare_text"
                android:textSize="10dp"
                android:textColor="#666666"
                android:layout_alignTop="@+id/fl_twiittershare_text"
                android:layout_toRightOf="@+id/imageView10"
                android:layout_toEndOf="@+id/imageView10"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="0dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Share on Twitter"
                android:id="@+id/fl_twiittershare_text"
                android:layout_below="@+id/creatorProfileImage"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:textSize="10dp"
                android:textColor="#666666"
                android:layout_marginRight="70dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="5dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView10"
                android:background="@drawable/fb"
                android:layout_alignTop="@+id/imageView11"
                android:layout_toRightOf="@+id/creatorProfileImage"
                android:layout_toEndOf="@+id/creatorProfileImage"
                android:layout_marginLeft="-10dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView11"
                android:background="@drawable/twr"
                android:layout_below="@+id/linearLayout3"
                android:layout_toLeftOf="@+id/fl_twiittershare_text"
                android:layout_toStartOf="@+id/fl_twiittershare_text" />

            <viewstyles.RoundedViewStyle
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/creatorProfileImage"
                android:background="@drawable/leaderbord_profile_icon"
                android:layout_marginLeft="10dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp" />
        </RelativeLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:src="@drawable/divider" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="vertical"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="#f6f7f2">

                <fonts.BebasNeueBoldTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:id="@+id/textView71"
                    android:text="PLAYERS"
                    android:textColor="#347296"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="10dp"
                    android:textStyle="bold"
                    android:textSize="18dp"/>

                <fonts.BebasNeueBoldTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/pointsleaderboard"
                    android:id="@+id/textView72"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="10dp"
                    android:textColor="#347296"
                    android:textStyle="bold"
                    android:textSize="18dp"

                    />

            </RelativeLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/game_leaderboard_tv_winner"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:textColor="@color/header_green"
                    android:textSize="@dimen/default_heading_size"
                    android:textStyle="bold" />


            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gl_list_layout"
            >
            <ListView
                android:id="@+id/glb_ll_leader_board"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:layout_marginTop="5dp"
                >
            </ListView>


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/default_margin"
                android:orientation="horizontal"
                android:weightSum="2"
                android:layout_below="@+id/glb_ll_leader_board">


            </LinearLayout>


        </RelativeLayout>
    </LinearLayout>
    <Button
        android:id="@+id/lb_returntohome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:background="@drawable/signup_bar"
        android:layout_above="@+id/footer"
        android:layout_centerHorizontal="true"

        android:padding="@dimen/default_padding"
        android:text="Return to Home"
        android:textColor="@android:color/white"
        android:layout_marginBottom="10dp"/>

    <include
        android:id="@+id/footer"
        layout="@layout/footer"
        />
</RelativeLayout>